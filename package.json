{
  "name": "react-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "turbo run lint",
    "clean": "turbo daemon clean",
    "build": "turbo run build",
    "build:ui-kit": "turbo run build --filter=ui-kit",
    "build:shared": "turbo run build --filter=shared",
    "build:icons": "turbo run build --filter=icons --force",
    "dev": "turbo run dev"
  },
  "devDependencies": {
    "clsx": "2.1.1",
    "eslint": "8.57.0",
    "eslint-config-frontend": "*",
    "turbo": "2.0.6",
    "vite-plugin-svgr": "4.2.0"
  },
  "workspaces": [
    "/apps/*",
    "packages/*"
  ],
  "packageManager": "npm@9.6.7"
}
